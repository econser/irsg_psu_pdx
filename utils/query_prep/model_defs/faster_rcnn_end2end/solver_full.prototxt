train_net: "<MODEL_DEF_DIR>/<MODEL_NAME>/faster_rcnn_end2end/train_full.prototxt"
base_lr: 0.001
lr_policy: "step"
gamma: 0.1
stepsize: 50000
display: 20
average_loss: 100
momentum: 0.9
weight_decay: 0.0005
# We disable standard caffe solver snapshotting and implement our own snapshot
# function
snapshot: 0
# We still use the snapshot prefix, though
snapshot_prefix: "<MODEL_NAME>_vgg_cnn_m_1024_faster_rcnn"
